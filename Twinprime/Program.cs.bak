using System;

namespace Twinprime
{
    class Program
    {
        static void Main(string[] args)
        {
                    for (int i = 0; i <= 50; i++) {  
                if (isPrime(i) == true && isPrime(i + 2) == true) {  
                    Console.WriteLine(i + "," + (i + 2));  
                }
                    }
                    
        }
            static bool isPrime(int n)  
    { 
        if (n <= 1) return false; 
        if (n <= 3) return true; 
        if (n % 2 == 0 || n % 3 == 0)  
            return false; 
        for (int i = 5; i * i <= n; i = i + 6) 
            if (n % i == 0 || n % (i + 2) == 0) 
                return false; 
        return true;
    }
    }
}
